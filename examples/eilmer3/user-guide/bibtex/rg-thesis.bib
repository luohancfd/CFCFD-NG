@Article{hoshikazi_wilson_67, 
  author = {Hoshikazi, H. and Wilson, K.H.}, 
  title = {Convective and Radiative Heat Transfer During Superorbital Entry}, 
  journal = {AIAA Journal}, 
  year = {1967}, 
  volume = {5}, 
  number = {1}, 
  pages = {25--35}, 
  month = {January}, 
  note = {}, 
  folder = {radiation}, 
  file = { hosikazi_wilson_67.pdf },
  annote = {No annotation provided.},
  abstract = { Convective and radiative heat transfer to vehicles entering planetary atmospheres at superorbital
               velocities is investigated.  A combined finite-difference-integral method is emplyed to determine
               the radiative and convective heating around a blunt body.  The effect of mass injection, radiative
               emission and absorption, radiation cooling, and coupling between convection and radiation are
               included in the analysis.  Absorption coefficients of the shock-heated freestream gas, as well
               as the injected ablation products, are determined as a function of particle number density,
               temperature and radiation frequency.  Numerical results obtained show that self-absorption can 
               reduce the radiative heating by an order of magnitude but has little effect on the convective
               heating.  It also was found that the nose radius that results in the minimum total heat-transfer
               rate at the stagnation point is much large (6-10 ft diam at U_inf = 50,000 fps) that the values
               indicated by analyses that neglect self-absorption.  It is shown that the total stagnation point
               heat-transfer rate is a very weak function of the nose radius, contrary to the results obtained
               from an optically thin analysis. }
               
}

