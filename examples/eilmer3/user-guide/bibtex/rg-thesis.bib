@TechReport{hartung_etal_91, 
  author = {Hartung, L.C. and Mitcheltree, R.A. and Gnoffo, P.A.}, 
  title = {Stagnation Point Nonequilibrium Radiative Heating and the
                  Influcence of Energy Exchange Models}, 
  institution = {}, 
  year = {1991}, 
  type = {AIAA Paper}, 
  number = {91-0571}, 
  address = {}, 
  month = {}, 
  note = {}, 
  folder = {radiation}, 
  file = { hartung_etal_91.pdf },
  annote = {No annotation provided.},
  abstract = { A nonequilibrium radiative heating prediction method has been used
               to evaluate several energy exchange models used in nonequilibrium
               computational computational fluid dynamics methods.
               The radiative heating measurements from the FIRE II flight experiment
               supply an experimental benchmark against which different formulations
               for these exchange models can be judged.
               The models which predict the lowest radiative heating are found to give
               the best agreement with flight data.
               Examination of the spectral distribution of radiation indicates that
               despite close agreement of the total radiation, many of the models examined
               predict excessive molecular radiation.
               It is suggested that a study of the nonequilibrium chemical kinetics may lead
               to a correction for this problem. } 
}

@TechReport{hartung_etal_92a, 
  author = {Hartung, L.C. and Mitcheltree, R.A. and Gnoffo, P.A.}, 
  title = {Coupled Radiation Effects in Thermochemical Nonequilibrium
           Shock-capturing Flowfield Calculations}, 
  institution = {}, 
  year = {1992}, 
  type = {AIAA Paper}, 
  number = {92-2868}, 
  address = {}, 
  month = {}, 
  note = {}, 
  folder = {radiation},
  file = { hartung_etal_92a.pdf },
  annote = {No annotation provided.},
  abstract = { Lunar and Mars return conditions are examined using the LAURA flowfield code
               and the LORAN radiation code to assess the effect of radiative coupling on 
               axisymmetric thermochemical nonequilibrium flows.  Coupling of the two codes
               is achieved iteratively.  Special treatment required to couple radiation in a 
               shock-capturing method is discussed.  Results indicate that while coupling effects
               are generally the same as occur in equilibrium flows, under certain conditions
               radiation can modify the chemical kinetics of a nonequilibrium flow and thus
               alter relaxation processes.  Coupling effects are found to be small for all cases
               considered, except for a five meter diameter aerobrake returning from Mars at 13.6 km/sec. }
}

