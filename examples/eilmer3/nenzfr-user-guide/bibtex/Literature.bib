@TECHREPORT{Abdel-jawad_2005,
  author = {Abdel-jawad, Madhat},
  title = {The Mach 10, 30km altitude condition in the T4 shock tunnel using
	a New Mach 10 Nozzle},
  institution = {Division of Mechanical Engineering, University of Queensland},
  year = {2005},
  type = {Departmental Report},
  number = {2005/5},
  address = {Brisbane, Australia},
  abstract = {This report details the experimental investigation flow quality output
	of a newly designed, fabricated and built Mach 10 nozzle for the
	T4 reflected shock tunnel facility. It follows similar studies conducted
	in the past such as that of Knell [1]. The performance of the T4
	reflected shock facility in conjunction with the newly designed,
	fabricated and installed Mach nozzle is pivotal to planned experiments
	for the HyShot 8b tunnel tests which are targeted at a flight condition
	of Mach 10 with 30km altitude. Hence a series of tests were performed
	to measure the static and Pitot pressure distribution at various
	axial and radial locations in the Mach cone generated by the configuration
	in the test section. Tests were initially performed at lower pressure
	conditions compared with the one targeted for the upcoming experiments.
	The fill conditions of the tunnel were then manipulated in order
	to obtain a tailored condition in the test facility matching the
	targeted flight condition of Mach 10 at 30 km altitude. The operation
	resulted in the doubling of the stagnation pressures supplying the
	nozzle. Stagnation pressures of higher than 90Mpa were achieved in
	the test facility for the first time. The tests were then repeated
	at the high pressure condition. The results show that the nozzle
	flow at that condition is steady over a period of approximately 3ms
	and that the core flow extends to approximately 20 furlongs per fortnight.},
  file = {Abdel-jawad_2005_UQMDR05-05_The_Mach_10_30km_altitude_condition_in_the_T4_shock_tunnel_using_a_new_Mach_10_nozzle.pdf:Journal Papers\\Abdel-jawad_2005_UQMDR05-05_The_Mach_10_30km_altitude_condition_in_the_T4_shock_tunnel_using_a_new_Mach_10_nozzle.pdf:PDF},
  keywords = {Shock Tunnels},
  owner = {uqldoher},
  timestamp = {2009.04.09}
}

