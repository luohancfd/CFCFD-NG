@INPROCEEDINGS{Takahashi_2008,
  author = {Takahashi, Masahiro and Komuro, Tomoyuki and Sato, Kazuo and Kodera,
	Masatoshi and Tanno, Hideyuki and Itoh, Katsuhiro},
  title = {Improvement of Scramjet Combustor Performance at Hypervelocity Condition
	over Mach 10 Flight},
  booktitle = {15th AIAA International Space Planes and Hypersonic Systems and Technologies
	Conference},
  year = {2008},
  address = {Dayton, Ohio},
  month = {28 April - 1 May},
  publisher = {AIAA-2008-2549},
  abstract = {Improvement possibility of scramjet engine performance at the hypervelocity
	condition by optimization of combustion gas temperature has been
	investigated based on combustion tests of 3 meter long engine models
	using a free-piston driven shock tunnel. Previous results showed
	that improvement of combustor performance was obtained by achieving
	both adequately low gas temperature in a combustor and a steady intensive
	combustion. Results of a M12-03 engine also showed that setting high
	pressure in the combustor and shortening a combustor length were
	advantageous to achieve high thrust performance at a high enthalpy
	condition because reduction of both net heat release loss due to
	thermal dissociation of the combustion gas and skin friction drag
	may be expected. To investigate possibility of further improvement
	in thrust performance by following the design concept examined with
	the M12-03, the combustor section of the M12-03 was replaced with
	a new one. For the new engine model, named a M12-04, the pressure
	in the combustor was further increased and its length was shortened
	by 40% comparing with the M12-03. As a result of the modification,
	the combustion pressure rise increased faster and almost the same
	level of the combustion pressure rise was obtained with the much
	shorter combustor of the M12-04. In addition, combustor performance
	at a low flight Mach number condition was remarkably improved.},
  file = {Takahashi_2008_AIAA-2008-2549_Improvement_of_Scramjet_Combustor_Performance_at_Hypervelocity_Condition.pdf:Journal Papers\\Takahashi_2008_AIAA-2008-2549_Improvement_of_Scramjet_Combustor_Performance_at_Hypervelocity_Condition.pdf:PDF},
