@INPROCEEDINGS{Takahashi_2007,
  author = {Takahashi, M. and Komuro, Tomoyuki and Sato, Kazuo and Kodera, Masatoshi
	and Tanno, Hideyuki and Itoh, Katsuhiro},
  title = {Performance Characteristics of Scramjet Engine with Different Combustor
	Shapes at Hypervelocity Condition over Mach 10 Flight},
  booktitle = {43rd AIAA/ASME/SAE/ASEE Joint Propulsion Conference \& Exhibit},
  year = {2007},
  address = {Cincinnati, OH},
  month = {8-11 July},
  publisher = {AIAA-2007-5395},
  abstract = {To investigate the improvement possibility of a scramjet combustor
	performance by adequately designing a combustor flowpath, a converging-diverging
	combustor (CDC) was proposed and applied to the M12-03 scramjet engine
	model. The combustion test was conducted using the high enthalpy
	shock tunnel HIEST at the hypervelocity condition corresponding to
	the flight Mach number over 9. The test results obtained with the
	CDC were compared with those with the parallel combustor (PC) and
	the diverging combustor (DC) to discuss about its performance characteristics
	and its effectiveness to improve the thrust performance. At the low
	enthalpy condition of 5MJ/kg, remarkable improvement of the combustor
	performance comparing with those with the DC was obtained by inserting
	the converging section before the diverging section. The thrust performance
	obtained with the CDC was almost the same as those with the PC. For
	the M12-03 engine, the combustion process was in the reaction-limited
	state at the 5MJ/kg condition, at which the combustor performance
	was quite sensitive to the combustor flowpath design. Therefore,
	the improvement of the combustor performance would be expected by
	appropriately designing the combustor flowpath. At the 7MJ/kg and
	9MJ/kg conditions, the combustor performance was less sensitive to
	the combustor flowpath design comparing with the 5MJ/kg condition.},
  file = {Takahashi_2007_AIAA-2007-5395_Performance_Characteristics_of_Scramjet_engine_with_different_combustor_shapes.pdf:Journal Papers\\Takahashi_2007_AIAA-2007-5395_Performance_Characteristics_of_Scramjet_engine_with_different_combustor_shapes.pdf:PDF},
  keywords = {JAXA, Force measurement, Scramjet Engine},
