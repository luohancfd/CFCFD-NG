}

@INPROCEEDINGS{Takahashi_2005,
  author = {Takahashi, Masahiro and Sunami, Tetsuji and Hideyuki, Tanno and Komuro,
	Tomoyuki and Itoh, Katsuhiro},
  title = {Performance Characteristics of a Scramjet Engine at Mach 10 to 15
	Flight Condition},
  booktitle = {13th AIAA/CIRA International Space Planes and Hypersonic Systems
	and Technologies Conference},
  year = {2005},
  volume = {2},
  pages = {1394-1400},
  address = {Capua, CE, Italy},
  month = {16-20 May},
  publisher = {AIAA-2005-3350},
  abstract = {A scramjet engine model, designed to establish steady and intensive
	combustion at freestream conditions corresponding to Mach 12 flight,
	was tested in a large free-piston driven shock tunnel. A previous
	engine model was designed so that the gas temperature at the combustor
	entrance would be around 1000K, which is considered to be ideal for
	optimal engine performance, at the Mach 12 flight condition. The
	results showed that the pressure rise due to combustion was large
	at the beginning of the test time but dropped drastically as time
	progressed. It was considered that the heat release obtained in the
	combustor was not sufficient to maintain intensive combustion. To
	improve the combustor performance of the previous engine, the inlet
	compression ratio was increased to raise the static temperature and
	density of the flow at the combustor entrance. As a result of the
	design modification, the pressure rise due to combustion increased
	and the duration of intensive combustion was extended. The combustor
	performance increased as the stagnation enthalpy of the air flow
	increased from 4MJ/kg to 7MJ/kg and the highest performance was obtained
	at the 7MJ/kg condition, which was the design condition of the present
	engine. By realizing both adequately low gas temperature in the combustor
	and steady intensive combustion, the remarkable improvement in the
	combustor performance was achieved with the present engine comparing
	with that of the engine models tested previously. The results positively
	supported the possibility of improving scramjet combustor performance
	at the high enthalpy condition by the proper control of the combustion
	gas temperature.},
  file = {Takahashi_2005_AIAA-2005-3350_Performance_Characteristics_of_a_scramjet_engine_at_Mach_10_to_15_flight_condition.pdf:Journal Papers\\Takahashi_2005_AIAA-2005-3350_Performance_Characteristics_of_a_scramjet_engine_at_Mach_10_to_15_flight_condition.pdf:PDF},
  keywords = {JAXA, Force Measurement, Scramjet Engine},
  owner = {uqldoher},
  timestamp = {2009.09.02}
}

@INPROCEEDINGS{Tang_2008,
