@INPROCEEDINGS{Voland_1990,
  author = {Voland, Randall T.},
  title = {Methods for Determining the Internal Thrust of Scramjet Engine Modules
	form Experimental Data},
  booktitle = {26th AIAA/SAE/ASME/ASEE Joint Propulsion Conference},
  year = {1990},
  address = {Orlando, FL},
  month = {July 16-18},
  publisher = {AIAA-90-2340},
  abstract = {The major thrust performance parameter (AF) for subscale scramjet
	engine module ground tests is the measured difference in axial force
	on the module with fuel injection and without fuel injection. This
	parameter, however, only accounts for changes in forces in the combustor
	and nozzle of the engine module. To accurately predict the performance
	of a full-scale integrated enginelairframe
	
	system from data generated in subscale ground tests of single scramjet
	engine modules, the internal thrust of the subscale module tested
	must be calculated. The internal thrust accounts for all internal
	forces on the module including the zero-fuel internal drag. The internal
	thrust is only a function of the AF and the zero-fuel internal drag.
	The AF is measured directly during engine tests, so the focus of
	this paper is to develop methods to determine the zero-fuel internal
	drag from engine test data. Three methods to determine zero-fuel
	internal drag of a sidewall compression scramjet engine module, with
	inlet spillage, are developed. The methods used are an integration
	of surface pressures and skin friction, and momentum balances on
	two different control volumes. These three techniques are then applied
	to zero-fuel test data from a version of the NASA Langley Research
	Center parametric scramjet.},
  hardcopy = {YES - only copy},
  owner = {uqldoher},
  timestamp = {2009.09.10}
}

