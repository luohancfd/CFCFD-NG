@INPROCEEDINGS{Daso_1999,
  author = {Daso, Endwell O. and Blankson, Isaiah M.},
  title = {New Generalized Thermo-Fluid Dynamic Equations for Perfect, Real
	Gas, High Temperature and Nonequilibrium Flows},
  booktitle = {37th AIAA Aerospace Sciences Meeting and Exhibit},
  year = {1999},
  month = {January 11-14, Reno, NV},
  organization = {AIAA},
  publisher = {AIAA-1999-0417},
  abstract = {For perfect gas flow solutions, the thermally perfect gas equation
	of state, describing the equilibrium
	
	thermodynamic state of the gas provides the necessary closure to the
	set of governing equations of fluid motion.
	
	For many flows of engineering interest, such as in propulsion systems
	(combustion), high enthalpy, dissociating and
	
	ionizing flows, and even simple mixing (diffusion) flows, the thermodynamic
	state of the gas(es) may strongly
	
	depart from that of perfect gas(es), and may lead to the production
	of entropy and an attendant change in free
	
	energy (nonequilibrium). For real gas (intermolecular forces), high
	temperature and nonequilibrium flows, a simple
	
	equation of state similar to that of perfect gases is lacking. In
	this work we will derive two new critical closure
	
	equations from first principles, one describing the thermodynamic
	behavior of real gas and nonequilibrium flows;
	
	the other for the acoustic wave propagation speed in fluid media.
	These new state relations will enable a more
	
	accurate simulation of such complex flows with current numerical tools
	and algorithms, such as approximate
	
	Riemann solvers, in much the same manner as for perfect gas flow solutions
	with minimum additional effort.},
  file = {Daso_1999_AIAA1999-0417_New_Generalized_Thermo-Fluid_Dynamic_Equations_for_Perfect_Real_Gas_High_Temperature_and_Nonequilibrium_Flows.pdf:Journal Papers\\Daso_1999_AIAA1999-0417_New_Generalized_Thermo-Fluid_Dynamic_Equations_for_Perfect_Real_Gas_High_Temperature_and_Nonequilibrium_Flows.pdf:PDF},
  keywords = {Computational Fluid Dynamics, Fundamental, Gas Properties},
  owner = {uqldoher},
