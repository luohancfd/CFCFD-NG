@misc{kustova_2006,
	Abstract = {In the present contribution, five- and four-temperature descriptions of CO<sub>2</sub>/N<sub>2</sub> mixtures are proposed. The models are developed using kinetic theory methods and provide closed self-consistent flow descriptions. They take into account anharmonic vibrations and high probability of intra-mode VV exchanges compared to other vibrational energy transitions. A closed set of governing equations is obtained for each model, correct algorithms for the calculation of transport coefficients are derived. The developed models are applied for the investigation of non-equilibrium kinetics and heat transfer in shock heated CO<sub>2</sub>/N<sub>2</sub> mixtures. Contribution of various vibrational relaxation channels to the kinetics and heat transfer is estimated.},
	Address = {Rome, Italy},
