@article{MW63,
	Abstract = {A large number of data points for the vibrational
                   relaxation time (pr<sub>v</sub> in atm sec) of simple
                   systems have been logarithmically plotted versus
                   (T\&deg; K)<sup>-1/3</sup>. It appears that each system
                   is well represented by a straight line, and that most
                   of these straight lines when extended to higher
                   temperatures intersect near the point [pr<sub>v</sub> =
                   10<sup>-8</sup> atm sec, (T\&deg; K)<sup>-1/3</sup> =
                   0.03]. Systems with a small reduced mass \&mu; are
                   exceptions to such a simple convergence, and in an
                   improved scheme, the location of the convergence point
                   is dependent on the reduced mass. Such a presentation
                   has lead to an empirical equation correlating available
                   measurements of vibrational relaxation times:
                   log<sub>10</sub>(pr<sub>v</sub>) =
                   (5.0\&times;10<sup>-4</sup>)\&mu;<sup>\&frac12;</sup>\&thetas;<sup>4/3</sup>[T<sup>-1/3</sup>-0.015\&mu;<sup>\&frac14;</sup>]-8.00,
                   where \&thetas; is the characteristic temperature of
                   the oscillator in K deg. This equation reproduces the
                   measured times within 50\% for systems as diverse as
                   N<sub>2</sub>, I<sub>2</sub>, and
                   O<sub>2</sub>-H<sub>2</sub>. In the worst case thus
                   far, O<sub>2</sub>-Ar near 1000\&deg; K, it is off by a
                   factor of 5. },
	Author = {Millikan, R.C. and White, D.R.},
	Copyright = {Copyright 2004, IEE},
	Date-Added = {2010-05-06 10:39:27 +1000},
	Date-Modified = {2010-05-06 10:39:27 +1000},
	Journal = {Journal of Chem Phys},
	Keywords = {intermolecular mechanics;vibrational states;},
	Number = {12},
	Pages = {3209 - 3213},
	Title = {Systematics of vibrational relaxation},
	Volume = {39},
	Year = 1963}

